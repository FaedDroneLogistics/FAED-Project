<?xml version="1.0"?>
<project type="activity">
  <name>Routable Input Python Activity Example</name>
  <description>
    An example of a Routable Interactive Spaces Activity written in Python that
    is an input for a route.
  </description>

  <identifyingName>
    interactivespaces.example.activity.routable.input.python
  </identifyingName>
  <version>1.0.0</version>
  
  <activity type="script">
    <name>interactivespacesExampleActivityRoutableInputPython</name>
    <executable>ExampleInputRoutablePythonActivity.py</executable>

    <configuration>
      <property name="space.activity.ros.node.name">
        <value>
          example/routable/input
        </value>
      </property>
      <property name="space.activity.routes.inputs" value="input1" />
      <property name="space.activity.route.input.input1">
        <value>
          /example/routable/channel1
        </value>
      </property>

      <property name="space.activity.log.level" value="info" />
    </configuration>
  </activity>
</project>
